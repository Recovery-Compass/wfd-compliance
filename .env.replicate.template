# Replicate API Configuration
VITE_REPLICATE_API_TOKEN=your_replicate_api_token_here

# Model Selection (Strategic Defaults)
# Image Generation - Google Imagen 4 Fast (cost-effective, high quality)
VITE_REPLICATE_IMAGE_MODEL=google/imagen-4-fast

# OCR/Document Processing - Datalab Marker (PDF to markdown)
VITE_REPLICATE_OCR_MODEL=datalab-to/marker

# Image Upscaling - Crystal Upscaler (portraits/faces)
VITE_REPLICATE_UPSCALE_MODEL=philz1337x/crystal-upscaler

# Video Generation - Google Veo 3.1 Fast (cost-effective, fast)
VITE_REPLICATE_VIDEO_MODEL=google/veo-3.1-fast

# Video Generation Pro - ByteDance Seedance-1-Pro (high quality, 480p/1080p)
VITE_REPLICATE_SEEDANCE_MODEL=bytedance/seedance-1-pro

# Text Extraction - Datalab OCR (multi-language support)
VITE_REPLICATE_TEXT_EXTRACT_MODEL=datalab-to/ocr
